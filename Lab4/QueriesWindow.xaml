<Window x:Class="Lab4.QueriesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab4"
        mc:Ignorable="d"
        Title="QueriesWindow" Height="130" Width="250" ResizeMode="NoResize">
    <Grid>
        <ComboBox Name="QueriesComboBox"  SelectedValuePath="Content" HorizontalAlignment="Center" Height="20" Width="150" Margin="0,0,0,30">
        </ComboBox>
        <StackPanel Margin="0,40,0,0" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="Застосувати" Width="70" Height="30" Margin="0,0,10,0" Click="SaveButton_Click"/>
            <Button Content="Скасувати" Width="70" Height="30" Click="CancelButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
